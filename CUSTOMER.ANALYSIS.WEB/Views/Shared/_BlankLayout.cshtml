<!doctype html>
<html lang="en">

<head>

    <meta charset="utf-8" />
    <title>@ViewBag.Title | Analisis Cliente Intercom</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Servicio de reparación y mantenimiento" name="description" />
    @*<meta content="Themesbrand" name="author" />*@
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/assets/images/frio-mar/isotipo.png">

    <!-- Bootstrap Css -->
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Icons Css -->
    <link href="~/assets/css/icons.min.css" rel="stylesheet" />
    <!-- App Css-->
    <link href="~/assets/css/app.min.css" rel="stylesheet" />
     <!--CSS Marketing-->
    <link href="~/assets/css/StyleMarketing.css" rel="stylesheet" type="text/css" />
    <!--CSS Font Poppins-->
    <link href="~/assets/css/font.css" rel="stylesheet" type="text/css" />
    <!-- sweetalert2 -->
    <link href="~/assets/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css" />

    @RenderSection("styles", required: false)
    <link rel="manifest" href="~/manifest.json">
    <meta name="theme-color" content="#2196f3" />
</head>

<body>

    @RenderBody()

    <!-- JAVASCRIPT -->
    <script src="~/assets/libs/jquery/jquery.min.js"></script>
    <script src="~/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/libs/metismenu/metisMenu.min.js"></script>
    <script src="~/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="~/assets/libs/node-waves/waves.min.js"></script>


    <!-- owl.carousel js -->
    <script src="~/assets/libs/owl.carousel/owl.carousel.min.js"></script>
    <!-- auth-2-carousel init -->
    <script src="~/assets/js/pages/auth-2-carousel.init.js"></script>
    <!-- two-step-verification js -->
    <script src="~/assets/js/pages/two-step-verification.init.js"></script>

    <!-- sweetalert2 -->
    <script src="~/assets/libs/sweetalert2/sweetalert2.min.js"></script>

    @RenderSection("scripts", required: false)
    <!-- App js -->
    <script src="~/assets/js/app.js"></script>


</body>
</html>